<h1>Individuos</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Plantacion</th>                <td>{{ entity.plantacion }}</td>        </tr>
        <tr>
            <th>Coordenadax</th>                <td>{{ entity.coordenadax }}</td>        </tr>
        <tr>
            <th>Coordenaday</th>                <td>{{ entity.coordenaday }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('individuos') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('individuos_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('individuos_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
